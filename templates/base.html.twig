<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>
      {% block title %}Welcome!
      {% endblock %}
    </title>
    {% block style %}
      <meta charset="utf-8">
      <meta http-equiv="x-ua-compatible" content="/ie=edge">

      <meta name="description" content="Hybridcore.eu">
      <meta name="viewport" content="width=device-width, initial-scale=1">

      <link rel="shortcut icon" type="/image/x-icon" href="/favicon.ico">
      <meta property="og:image" content="/img/hybrid_logo_transparent.png"/>
      <meta property="og:image:width" content="100"/>
      <meta property="og:image:height" content="100"/>
      <style>
      /* Scroll bar */
      ::-webkit-scrollbar {
        width: 10px;
        border-radius: 10px;
      }

      ::-webkit-scrollbar-track {
        background: #f1f1f1;
        border-radius: 10px;
      }

      ::-webkit-scrollbar-thumb {
        background: #888;
        border-radius: 10px;
      }

      ::-webkit-scrollbar-thumb:hover {
        background: #555;
        border-radius: 10px;
      }
    </style>

    <!-- CSS here -->
    <link rel="stylesheet" href="/css/fontawesome/fontawesome.min.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/css/magnific-popup.css">
    <link rel="stylesheet" href="/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/themify-icons.css">
    <link rel="stylesheet" href="/css/nice-select.css">
    <link rel="stylesheet" href="/css/flaticon.css">
    <link rel="stylesheet" href="/css/gijgo.css">
    <link rel="stylesheet" href="/css/animate.css">
    <link rel="stylesheet" href="/css/slick.css">
    <link rel="stylesheet" href="/css/slicknav.css">
    <link rel="stylesheet" href="/css/style.css">
  {% endblock %}
</head>
<body>
  {% block navbar %}
    <header>
      <div class="header-area ">
        <div id="sticky-header" class="main-header-area">
          <div class="container">
            <div class="header_bottom_border">
              <div class="row align-items-center">
                <div class="col-xl-3 col-lg-3">
                  <div class="logo">
                    <a href="/">
                      <img src="/img/logo.png" height="50px" alt="Hybrid Core ">
                      <span class="text-white d-none d-sm-inline">Hybrid Core</span>
                    </a>
                  </div>
                </div>
                <div class="col-xl-9 col-lg-9">
                  <div class="main-menu d-none d-lg-block">
                    <nav>
                      <ul id="navigation" class="text-right">
                        
                        {% if app.user %}
                          <li>
                            <a class="text-white h3" style="cursor: pointer" href="#">
                              <i style="font-size: 14px;" class="fas fa-user-shield"></i>
                              <i style="margin-left: 5px" class="ti-angle-down"></i>
                            </a>
                            <ul class="submenu">
                              <li style="word-break: break-all;">
                                {{app.user.getFullname}}
                              </li>
                              {% if is_granted('ROLE_ADMIN') %}
                                <li>
                                  <a href="{{ path('easyadmin') }}">Dashboard</a>
                                </li>
                              {% else %}
                                <li class="bg-danger mx-0 p-2">
                                  You are not authorized to see dashboard yet. Please contact to Timu√ßin.
                                </li>
                              {% endif %}
                              <li>
                                <a href="{{ path('app_logout') }}">logout</a>
                              </li>
                            </ul>
                          </li>
                        {% endif %}
                        <li>
                          <a class="active" href="/">Home</a>
                        </li>
                        <li>
                          <a href="#">Digital Platforms<i style="margin-left: 5px" class="ti-angle-down"></i>
                          </a>
                          <ul class="submenu">
                            <li>
                              <a href="/digital-platforms/smart-navigator" target="_blank">Smart Navigator</a>
                            </li>
                            <li>
                              <a href="/digital-platforms/smart-sonar" target="_blank">Smart Sonar</a>
                            </li>
                            <li>
                              <a href="/digital-platforms/smart-predictive-policing" target="_blank">Smart Predictive Policing</a>
                            </li>
                          </ul>
                        </li>
                        <li>
                          <a class="" style="cursor: pointer" href="#">Demos<i style="margin-left: 5px" class="ti-angle-down"></i>
                          </a>
                          <ul class="submenu">
                            <li>
                              <a href="/demos/smart-sonar" target="_blank">Smart Sonar</a>
                            </li>
                            <li>
                              <a href="/demos/covid-19" target="_blank">Covid-19 Dashboard</a>
                            </li>
                          </ul>
                        </li>
                        <li>
                          <a class="" href="/team" style="cursor: pointer">Team</a>
                        </li>
                        <li>
                          <a class="" href="/blog" style="cursor: pointer">Blog</a>
                        </li>
                        <li>
                          <a class="" style="cursor: pointer" onclick='$("#contact_form").modal("show")'>Contact</a>
                        </li>
                      </ul>
                    </nav>
                  </div>
                </div>
                <div class="col-12">
                  <div class="mobile_menu d-block d-lg-none"></div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
  {% endblock %}

  {% block body %}{% endblock %}

  {% block contact_form %}
    <div class="modal fade" id="contact_form" tabindex="-1" role="dialog" aria-labelledby="contact_formLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="p-2 rounded-top">
          <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-content">
          <div class="modal-body">
            <div class="row justify-content-center">
              <div class="col-12 text-center">
                <h2 class="contact-title text-center mb-0">Get in Touch</h2>
                <strong>
                  <a href="mailto:hybridcorealpha@gmail.com" target="_blank" class="text-primary">hybridcorealpha@gmail.com
                    <i class="far fa-envelope-open"></i>
                  </a>
                </strong>
                <h2 class="contact-title text-center">or</h2>
              </div>
              <div class="col-12">
                <form class="form-contact contact_form" method="post" id="contact">
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="form-group">
                        <input class="form-control valid rounded" name="name" id="name" type="text" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter your name'" placeholder="Name*" required>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="form-group">
                        <input class="form-control valid rounded" name="email" id="email" type="email" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter email address'" placeholder="Email address*" required>
                      </div>
                    </div>
                    <div class="col-12">
                      <div class="form-group">
                        <input class="form-control rounded" name="company" id="company" type="text" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter company name'" placeholder="Company name">
                      </div>
                    </div>
                    <div class="col-12">
                      <div class="form-group mb-3">
                        <textarea class="form-control w-100 rounded" name="message" id="message" cols="30" rows="9" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Message'" placeholder="Message*" required></textarea>
                      </div>
                    </div>
                  </div>
                  <div class="form-group my-0">
                    <button type="submit" id="submit_button" class="button button-contactForm boxed-btn rounded">Send</button>
                  </div>
                  <p id="form_message" style="color:red;margin:0"></p>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  {% endblock %}
  {% block footer %}
    <footer class="footer">
      <div class="footer_top py-5">
        <div class="container">
          <div class="row justify-content-center">
            <div class="col-xl-4 col-md-6 col-lg-4">
              <div class="footer_widget">
                <div class="footer_logo text-center">
                  <img src="/img/hybrid_logo_transparent.png" width="200px" alt="">
                  <span class="genric-btn info circle text-black" onclick='$("#contact_form").modal("show")'>Contact us</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </footer>
  {% endblock %}

  {% block script %}
    <script src="/js/vendor/modernizr-3.5.0.min.js"></script>
    <script src="/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="/js/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/isotope.pkgd.min.js"></script>
    <script src="/js/ajax-form.js"></script>
    <script src="/js/waypoints.min.js"></script>
    <script src="/js/jquery.counterup.min.js"></script>
    <script src="/js/imagesloaded.pkgd.min.js"></script>
    <script src="/js/scrollIt.js"></script>
    <script src="/js/jquery.scrollUp.min.js"></script>
    <script src="/js/wow.min.js"></script>
    <script src="/js/nice-select.min.js"></script>
    <script src="/js/jquery.slicknav.min.js"></script>
    <script src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/plugins.js"></script>
    <script src="/js/gijgo.min.js"></script>
    <script src="/js/slick.min.js"></script>
    <script src="/js/jquery.form.js"></script>

    <script src="/js/main.js"></script>
  {% endblock %}
</body></html>
